<%= simple_form_for([@user, @booking]) do |f| %>
<%= f.input :start_time, as: :datetime, html5: true %>
<%= f.input :end_time, as: :datetime, html5: true %>
<%= f.input :client_id, collection: Client.all%>
<%= f.submit class: "nav-button col-12"%>
<% end %>
<%= link_to "Back to dashboard", dashboard_path(@user), class: "nav-button col-12" %>
